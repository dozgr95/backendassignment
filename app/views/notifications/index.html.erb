<h1><%= @adminviewtitle %></h1>
<% @notifications.each do |notification| %>
    Notification Nr. <%=notification.id%>: 
    <h3><%= notification.title %></h3>
    <p><%= notification.description %><p>
    <p>Created At: <%= notification.created_at %> <p>
    <p>Updated At: <%= notification.updated_at %> </p>
    <p>New (unread): <%= notification.active %></p><br><br>
<% end %>
